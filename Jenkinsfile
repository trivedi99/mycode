pipeline {
    agent any 
    stages {
        stage('Build') {
            steps {
                echo 'Build From SCM!' 
            }
        }
 	stage('Deply') {
            steps {
                echo 'Deploy From SCM!'
            }
        }
	stage('Release') {
            steps {
                echo 'Release From SCM!'
            }
        }   
   }
}
